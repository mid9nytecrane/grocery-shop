{
    "version": 2,
    "builds": [
        {
            "src": "grocery_shop/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb" , "pythonVersion": "3.9" }

        },
        {
            "src":"build_static.sh",
            "use":"@vercel/static-build"
        }
        
    ],

    "routes": [
        { "src": "/(.*)", "dest": "grocery_shop/wsgi.py" },
        {"src":"/media/(.*)","dest":"/media/$1"},
        {"src":"/static/(.*)","dest":"/static/$1"}
    ],

    "env":{
        "DEBUG":"False",
        "PYTHONPATH":"/vercel/path0/",
        "ALLOWED_HOSTS":"www.meisallahbusinesscenter.com,meisallahbusinesscenter.com,.vercel.app,.now.sh"
    }
}